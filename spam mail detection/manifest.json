{
    "manifest_version": 3,
    "name": "Social media spam detection",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "contextMenus",
        "storage",
        "activeTab",
        "notifications"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
